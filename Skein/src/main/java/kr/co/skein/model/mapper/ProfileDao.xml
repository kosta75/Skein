<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.skein.model.dao.AccountDao">
	
	<!-- 사용자 프로필 업데이트 -->
	<insert id="updateProfile" parameterType="kr.co.skein.model.vo.ProfileCommand">
		INSERT INTO 
	</insert>
	

	<select id="getAccountEmail"  parameterType="kr.co.skein.model.vo.Member" resultType="String">
		SELECT email FROM Members WHERE Email = #{email} AND FullName = #{fullName} AND Birthday = #{birthday}
	</select>
	
	
	<update id="updateAccountPassword" parameterType="kr.co.skein.model.vo.Member">
		UPDATE Members
		SET password = #{password}
		WHERE email = #{email}
	</update>
	

	
</mapper>