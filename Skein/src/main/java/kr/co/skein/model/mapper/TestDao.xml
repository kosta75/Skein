<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.skein.model.dao.TestDao">
	
	<insert id="writeReg" parameterType="kr.co.skein.model.vo.TestCommand">
		<selectKey keyProperty="seq" resultType="Integer">
			SELECT NVL(MAX(seq), 0))+1 FROM Test
		</selectKey>
		INSERT INTO Test VALUES(#{seq}, #{content})
	</insert>
	
</mapper>
